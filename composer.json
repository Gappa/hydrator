{
	"name": "webchemistry/doctrine-hydration",
	"type": "library",
	"require": {
		"webchemistry/strict-object": "~1.1"
	},
	"autoload": {
		"psr-4": {
			"WebChemistry\\DoctrineHydration\\": ["src/"]
		}
	},
	"autoload-dev": {
		"psr-4": {
			"WebChemistry\\Test\\": ["tests/classes/"],
			"": ["tests/entities/"]
		}
	},
	"minimum-stability": "dev",
	"require-dev": {
		"phpstan/phpstan": "^0.11.0@dev",
		"codeception/codeception": "2.5.x-dev",
		"doctrine/orm": "^2.6.2",
		"nette/di": "~2.4.14"
	},
	"scripts": {
		"stan": "vendor/bin/phpstan analyse src --level=7",
		"test": "vendor/bin/codecept run"
	}
}
